xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
<xsl:output method="html" indent="no" encoding="utf-8" media-type="text/html" doctype-public="-//W3C//DTD HTML 4.0//EN"/>
<xsl:variable name="cellsPerRow" select="3"/>
<xsl:variable name="maxItems" select="1000"/>
<xsl:param name="testcaserid"/>
<xsl:param name="loadAll"/>
<!-- Report Root Content -->
<xsl:template match="/report/*">
<div id="header-top">
<h1 id="logo">
<span title="Ranorex">
<em>Ranorex Report</em>
</span>
<span title="Ranorex">...</span>
</h1>
<h1 id="logo">...</h1>
<div id="small-nav"/>
</div>
<div id="header-top">...</div>
<div id="page">
<div id="container">
<div id="Content">
<xsl:choose>
<!-- Testsuit Template -->
<xsl:when test="./activity[@type='test-suite' or @type='test-case' or @type='smart-folder' or @type='iteration-container' or @type='entry-container']">
<h1>
<xsl:choose>
<xsl:when test="./activity/@headertext">
<xsl:value-of select="./activity/@headertext"/>
</xsl:when>
<xsl:when test="./activity/@headertext">...</xsl:when>
<xsl:otherwise>Report for selected test item</xsl:otherwise>
</xsl:choose>
<xsl:choose>...</xsl:choose>
</h1>
<h1>...</h1>
<p>
<xsl:choose>
<xsl:when test="../@progress != '' ">
<div class="progress">
<img alt="progress" width="16" height="11" src="data:image/gif;base64,"/>
<span style="padding-left:5px;">Test in progress</span>
</div>
<div class="progress">...</div>
</xsl:when>
<xsl:when test="../@progress != '' ">...</xsl:when>
</xsl:choose>
<xsl:choose>...</xsl:choose>
</p>
<p>...</p>
<p>
<xsl:choose>
<xsl:when test="./activity[@type='test-suite']">
<xsl:value-of select="./activity/detail" disable-output-escaping="yes"/>
</xsl:when>
<xsl:when test="./activity[@type='test-suite']">...</xsl:when>
<xsl:otherwise>
<xsl:value-of select="./detail" disable-output-escaping="yes"/>
</xsl:otherwise>
<xsl:otherwise>...</xsl:otherwise>
</xsl:choose>
<xsl:choose>...</xsl:choose>
<xsl:value-of select="./activity/@runlabel"/>
</p>
<p>...</p>
<br/>
<!-- EXECUTION INFORMATION -->
<xsl:call-template name="executionInformation"/>
<xsl:variable name="itemCount">
<xsl:value-of select="count(.//item)"/>
</xsl:variable>
<xsl:variable name="itemCount">...</xsl:variable>
<div>
<!-- BUTTONS -->
<div>
<xsl:choose>
<xsl:when test="$itemCount < $maxItems">
<xsl:if test="not(@testentry-activity-type = 'testmodule')">
<input type="button" onclick="expandTestcases()" class="exButton" value="Expand test containers"/>
</xsl:if>
<xsl:if test="not(@testentry-activity-type = 'testmodule')">...</xsl:if>
<input type="button" onclick="expandDetails()" class="exButton" value="Expand details"/>
<input type="button" onclick="collapseAll()" class="exButton" value="Collapse all"/>
</xsl:when>
<xsl:when test="$itemCount < $maxItems">...</xsl:when>
<xsl:otherwise>
<xsl:if test="not(@testentry-activity-type = 'testmodule')">
<input type="button" onclick="expandTestcases()" class="exButton" value="Expand test containers" disabled="disabled"/>
</xsl:if>
<xsl:if test="not(@testentry-activity-type = 'testmodule')">...</xsl:if>
<input type="button" onclick="expandDetails()" class="exButton" value="Expand details" disabled="disabled"/>
<input type="button" onclick="collapseAll()" class="exButton" value="Collapse all" disabled="disabled"/>
<em style="color: #888; font-size: 12px;">Disabled due to performance reasons.</em>
</xsl:otherwise>
<xsl:otherwise>...</xsl:otherwise>
</xsl:choose>
<xsl:choose>...</xsl:choose>
</div>
<div>...</div>
<!-- PIECHART -->
<xsl:if test="not(@testentry-activity-type = 'testmodule')">
<h3 id="testCasesHeader">Test case result summary</h3>
<div id="testCasesPie">
<xsl:attribute name="totalsuccesstestcasecount">
<xsl:value-of select="@totalsuccesstestcasecount"/>
</xsl:attribute>
<xsl:attribute name="totalsuccesstestcasecount">...</xsl:attribute>
<xsl:attribute name="totalfailedtestcasecount">
<xsl:value-of select="@totalfailedtestcasecount"/>
</xsl:attribute>
<xsl:attribute name="totalfailedtestcasecount">...</xsl:attribute>
<xsl:attribute name="totalblockedtestcasecount">
<xsl:value-of select="@totalblockedtestcasecount"/>
</xsl:attribute>
<xsl:attribute name="totalblockedtestcasecount">...</xsl:attribute>
</div>
<div id="testCasesPie">...</div>
</xsl:if>
<xsl:if test="not(@testentry-activity-type = 'testmodule')">...</xsl:if>
</div>
<div>...</div>
<!-- Test Case Filter Checkboxes -->
<div style="margin-top: 20px;">
<xsl:call-template name="globalCategorySelector"> </xsl:call-template>
</div>
<div style="margin-top: 20px;">...</div>
<xsl:if test=".//item[@level='Warn']">
<div class="warnmessage">
<span class="ui-module-icon warn"/>
Warnings occurred. For additional information see the report of the individual modules, please.
</div>
<div class="warnmessage">...</div>
</xsl:if>
<xsl:if test=".//item[@level='Warn']">...</xsl:if>
<xsl:if test="./activity[@type='integration']">
<div class="integration-errormessage">
<span class="ui-module-icon error"/>
Integration service
<xsl:value-of select="./activity[@type='integration']/@integrationtype"/>
has encountered some errors.
<a href="#integrationsection">Click here to navigate to the integration section.</a>
</div>
<div class="integration-errormessage">...</div>
</xsl:if>
<xsl:if test="./activity[@type='integration']">...</xsl:if>
<ul id="treeList" class="ui-treeList">
<!-- PRE TEST SUITE BEGIN -->
<xsl:if test="./activity/preceding-sibling::item">
<li class="ui-treeList-item" id="pre-testsuite">
<h2>
Before Test Suite
<xsl:if test="./activity/preceding-sibling::item[@level='Warn']">
<span class="ui-module-icon warn"/>
</xsl:if>
<xsl:if test="./activity/preceding-sibling::item[@level='Warn']">...</xsl:if>
</h2>
<h2>...</h2>
<ul class="pre-testsuite">
<div class="module-container" style="display:block">
<div class="module-header">
<xsl:call-template name="levelFilterSelector"/>
</div>
<div class="module-header">...</div>
<div class="module-report">
<TABLE border="0" cellSpacing="0" class="reporttable">
<thead>
<th>
<b>Time</b>
</th>
<th>...</th>
<th>
<b>Level</b>
</th>
<th>...</th>
<th>
<b>Category</b>
</th>
<th>...</th>
<th>
<b>Message</b>
</th>
<th>...</th>
</thead>
<thead>...</thead>
<tbody>
<xsl:apply-templates select="./activity/preceding-sibling::item">
<xsl:with-param name="type">standalone</xsl:with-param>
</xsl:apply-templates>
<xsl:apply-templates select="./activity/preceding-sibling::item">...</xsl:apply-templates>
</tbody>
<tbody>...</tbody>
</TABLE>
<TABLE border="0" cellSpacing="0" class="reporttable">...</TABLE>
</div>
<div class="module-report">...</div>
</div>
<div class="module-container" style="display:block">...</div>
</ul>
<ul class="pre-testsuite">...</ul>
</li>
<li class="ui-treeList-item" id="pre-testsuite">...</li>
</xsl:if>
<xsl:if test="./activity/preceding-sibling::item">...</xsl:if>
<!-- PRE TEST SUITE END -->
<xsl:if test="./activity[@type='test-suite']/params/param">
<div class="dataparams dataparams-pre-testsuite">
<xsl:apply-templates select="./activity[@type='test-suite']/params"/>
</div>
<div class="dataparams dataparams-pre-testsuite">...</div>
</xsl:if>
<xsl:if test="./activity[@type='test-suite']/params/param">...</xsl:if>
<xsl:choose>
<xsl:when test="./activity[@type='test-suite']">
<xsl:apply-templates select="./activity/activity">
<xsl:with-param name="itemCount" select="$itemCount"/>
</xsl:apply-templates>
<xsl:apply-templates select="./activity/activity">...</xsl:apply-templates>
</xsl:when>
<xsl:when test="./activity[@type='test-suite']">...</xsl:when>
<xsl:otherwise>
<xsl:apply-templates select="./activity">
<xsl:with-param name="itemCount" select="$itemCount"/>
</xsl:apply-templates>
<xsl:apply-templates select="./activity">...</xsl:apply-templates>
</xsl:otherwise>
<xsl:otherwise>...</xsl:otherwise>
</xsl:choose>
<xsl:choose>...</xsl:choose>
<!-- POST TEST SUITE BEGIN -->
<xsl:if test="./activity/following-sibling::item">
<li class="ui-treeList-item" id="post-testsuite">
<h2>
After Test Suite
<xsl:if test="./activity/following-sibling::item[@level='Warn']">
<span class="ui-module-icon warn"/>
</xsl:if>
<xsl:if test="./activity/following-sibling::item[@level='Warn']">...</xsl:if>
</h2>
<h2>...</h2>
<ul class="pre-testsuite">
<div class="module-container" style="display:block">
<div class="module-header">
<xsl:call-template name="levelFilterSelector"/>
</div>
<div class="module-header">...</div>
<div class="module-report">
<TABLE border="0" cellSpacing="0" class="reporttable">
<thead>
<th>
<b>Time</b>
</th>
<th>...</th>
<th>
<b>Level</b>
</th>
<th>...</th>
<th>
<b>Category</b>
</th>
<th>...</th>
<th>
<b>Message</b>
</th>
<th>...</th>
</thead>
<thead>...</thead>
<tbody>
<xsl:apply-templates select="./activity/following-sibling::item">
<xsl:with-param name="type">standalone</xsl:with-param>
</xsl:apply-templates>
<xsl:apply-templates select="./activity/following-sibling::item">...</xsl:apply-templates>
</tbody>
<tbody>...</tbody>
</TABLE>
<TABLE border="0" cellSpacing="0" class="reporttable">...</TABLE>
</div>
<div class="module-report">...</div>
</div>
<div class="module-container" style="display:block">...</div>
</ul>
<ul class="pre-testsuite">...</ul>
</li>
<li class="ui-treeList-item" id="post-testsuite">...</li>
</xsl:if>
<xsl:if test="./activity/following-sibling::item">...</xsl:if>
<!-- POST TEST SUITE END -->
<!-- INTEGRATION SECTION BEGIN -->
<xsl:if test="./activity[@type='integration']">
<a name="integrationsection"/>
<li class="ui-treeList-item" id="integration-testsuite">
<h2>
<xsl:value-of select="./activity[@type='integration']/@integrationtype"/>
Report Section
</h2>
<h2>...</h2>
<ul class="pre-testsuite">
<div class="module-report">
<TABLE border="0" cellSpacing="0" class="Integration">
<thead>
<th>
<b>Time</b>
</th>
<th>...</th>
<th>
<b>Level</b>
</th>
<th>...</th>
<th>
<b>Category</b>
</th>
<th>...</th>
<th>
<b>Message</b>
</th>
<th>...</th>
</thead>
<thead>...</thead>
<tbody>
<xsl:apply-templates select="./activity[@type='integration']/item">
<xsl:with-param name="type">standalone</xsl:with-param>
</xsl:apply-templates>
<xsl:apply-templates select="./activity[@type='integration']/item">...</xsl:apply-templates>
</tbody>
<tbody>...</tbody>
</TABLE>
<TABLE border="0" cellSpacing="0" class="Integration">...</TABLE>
</div>
<div class="module-report">...</div>
</ul>
<ul class="pre-testsuite">...</ul>
</li>
<li class="ui-treeList-item" id="integration-testsuite">...</li>
</xsl:if>
<xsl:if test="./activity[@type='integration']">...</xsl:if>
<!-- INTEGRATION SECTION END -->
</ul>
<ul id="treeList" class="ui-treeList">...</ul>
</xsl:when>
<xsl:when test="./activity[@type='test-suite' or @type='test-case' or @type='smart-folder' or @type='iteration-container' or @type='entry-container']">...</xsl:when>
<!-- Standalone Recording Template -->
<xsl:otherwise>
<h1>
<xsl:value-of select="@module"/>
</h1>
<h1>...</h1>
<xsl:choose>
<xsl:when test="../@progress != '' ">
<div class="progress">
<img alt="progress" width="16" height="6" src="data:image/gif;base64,"/>
<span style="padding-left:5px;">Test in progress</span>
</div>
<div class="progress">...</div>
</xsl:when>
<xsl:when test="../@progress != '' ">...</xsl:when>
<xsl:otherwise>
<h2 class="module {@result}">
<span class="ui-status-icon"/>
<xsl:value-of select="@result"/>
</h2>
<h2 class="module {@result}">...</h2>
</xsl:otherwise>
<xsl:otherwise>...</xsl:otherwise>
</xsl:choose>
<xsl:choose>...</xsl:choose>
<xsl:call-template name="executionInformation"/>
<xsl:choose>
<xsl:when test=".//item">
<div style="margin-top: 20px;">
<xsl:call-template name="levelFilterSelector"/>
</div>
<div style="margin-top: 20px;">...</div>
<div class="module-report" style="margin:15px 0 40px 0; position:relative;">
<TABLE border="0" cellSpacing="0" class="reporttable">
<thead>
<tr>
<th>
<b>Time</b>
</th>
<th>...</th>
<th>
<b>Level</b>
</th>
<th>...</th>
<th>
<b>Category</b>
</th>
<th>...</th>
<th>
<b>Message</b>
</th>
<th>...</th>
</tr>
<tr>...</tr>
</thead>
<thead>...</thead>
<tbody>
<xsl:apply-templates select="./item">
<xsl:with-param name="type">standalone</xsl:with-param>
</xsl:apply-templates>
<xsl:apply-templates select="./item">...</xsl:apply-templates>
</tbody>
<tbody>...</tbody>
</TABLE>
<TABLE border="0" cellSpacing="0" class="reporttable">...</TABLE>
</div>
<div class="module-report" style="margin:15px 0 40px 0; position:relative;">...</div>
</xsl:when>
<xsl:when test=".//item">...</xsl:when>
<xsl:otherwise>
<!-- No Item. -->
</xsl:otherwise>
<xsl:otherwise>...</xsl:otherwise>
</xsl:choose>
<xsl:choose>...</xsl:choose>
</xsl:otherwise>
<xsl:otherwise>...</xsl:otherwise>
</xsl:choose>
<xsl:choose>...</xsl:choose>
</div>
<div id="Content">...</div>
<xsl:choose>
<xsl:when test="../@progress != '' ">
<div style="width:99%; text-align:center;padding-bottom: 10px;">
<div class="progress">
<img alt="progress" width="16" height="11" src="data:image/gif;base64,"/>
<span style="padding-left:5px;">Test in progress</span>
</div>
<div class="progress">...</div>
</div>
<div style="width:99%; text-align:center;padding-bottom: 10px;">...</div>
<div style="width:100%; text-align:center;padding-bottom: 10px;">
<input type="submit" onclick="window.location.reload(false)" class="hyperlinkButton" value="Click here to refresh"/>
</div>
<div style="width:100%; text-align:center;padding-bottom: 10px;">...</div>
</xsl:when>
<xsl:when test="../@progress != '' ">...</xsl:when>
</xsl:choose>
<xsl:choose>...</xsl:choose>
<div style="clear:both"/>
<div id="Footer"/>
</div>
<div id="container">...</div>
</div>
<div id="page">...</div>
</xsl:template>
<xsl:template match="/report/*">...</xsl:template>
<!-- TESTCASE AND CONTAINER TEMPLATE -->
<xsl:template match="activity[(@type='test-case' or @type='smart-folder' or @type='iteration-container' or @type='entry-container') and not(@result='Pending')]">
<xsl:param name="itemCount"/>
<xsl:variable name="activityclassname" select="concat(@type, ' ', @testentry-activity-type, ' ',@iteration-exectype, ' ', @activity-exectype)"/>
<li class="{@result} ui-treeList-item {$activityclassname}" id="container{@rid}">
<xsl:variable name="success" select="@childsuccesscount"/>
<xsl:variable name="failed" select="@childfailedcount"/>
<xsl:variable name="ignored" select="@childblockedcount"/>
<xsl:variable name="max" select="$success + $failed + $ignored"/>
<h2 class="{@result}" onclick="OnLoadContentDynamic('{@rid}','container',this);" onMouseOver="DisplayHoverMenu(this);" onMouseOut="HideHoverMenu(this)">
<!-- JUMP TO -->
<xsl:if test="./@testcontainerid">
<xsl:variable name="jumpToText">
<xsl:choose>
<xsl:when test="./@testentry-activity-type='testcase' or ./@type='test-case'">
<xsl:text>Jump to test case</xsl:text>
</xsl:when>
<xsl:when test="./@testentry-activity-type='testcase' or ./@type='test-case'">...</xsl:when>
<xsl:otherwise>
<xsl:text>Jump to smart folder</xsl:text>
</xsl:otherwise>
<xsl:otherwise>...</xsl:otherwise>
</xsl:choose>
<xsl:choose>...</xsl:choose>
</xsl:variable>
<xsl:variable name="jumpToText">...</xsl:variable>
<div class="controls-container testcase">
<a href="#" class="jump-to">
<metainfo id="{@testcontainerid}" type="{$activityclassname}"/>
<span class="ui-icon"/>
<xsl:value-of select="$jumpToText"/>
</a>
<a href="#" class="jump-to">...</a>
</div>
<div class="controls-container testcase">...</div>
</xsl:if>
<xsl:if test="./@testcontainerid">...</xsl:if>
<span class="ui-status-icon ui-treeList-toggle-child"/>
<span class="ui-icon {$activityclassname} ui-treeList-toggle-child"/>
<!-- DISPLAY NAME TEXT -->
<Span class="displayname {$activityclassname} ui-treeList-toggle-child">
<xsl:value-of select="./@displayName"/>
</Span>
<Span class="displayname {$activityclassname} ui-treeList-toggle-child">...</Span>
<xsl:if test="./@conditionstate='met'">
<span class="ui-module-icon conditionmet ui-treeList-toggle-child"/>
</xsl:if>
<xsl:if test="./@conditionstate='met'">...</xsl:if>
<xsl:if test="./@conditionstate='notmet'">
<span class="ui-module-icon conditionnotmet ui-treeList-toggle-child"/>
</xsl:if>
<xsl:if test="./@conditionstate='notmet'">...</xsl:if>
<!-- SHOW STATUS ICON - WARN, ERROR AND MAINTENANCEMODE -->
<xsl:if test=".//item[@level='Warn']">
<span class="ui-module-icon warn ui-treeList-toggle-child"/>
</xsl:if>
<xsl:if test=".//item[@level='Warn']">...</xsl:if>
<xsl:if test=".//item[@level='Error' or @level='Failure']">
<span class="ui-module-icon error ui-treeList-toggle-child"/>
</xsl:if>
<xsl:if test=".//item[@level='Error' or @level='Failure']">...</xsl:if>
<xsl:if test="@totalmaintenancemodecount > 0">
<span class="ui-module-icon MaintenanceMode" title="Maintenance mode"/>
</xsl:if>
<xsl:if test="@totalmaintenancemodecount > 0">...</xsl:if>
<!-- ITERATION INFO TEXT -->
<xsl:if test="@iteration-exectype or (@activity-exectype and @activity-exectype != 'execute')">
<span class="iterationinfo {$activityclassname} ui-treeList-toggle-child">
<xsl:choose>
<xsl:when test="@iteration-exectype = 'dataiteration'">
<xsl:text>Rows:</xsl:text>
</xsl:when>
<xsl:when test="@iteration-exectype = 'dataiteration'">...</xsl:when>
<xsl:when test="@iteration-exectype = 'runiteration'">
<xsl:text>Iterations:</xsl:text>
</xsl:when>
<xsl:when test="@iteration-exectype = 'runiteration'">...</xsl:when>
<xsl:when test="@activity-exectype = 'dataiteration'">
<xsl:text>Data Row:</xsl:text>
</xsl:when>
<xsl:when test="@activity-exectype = 'dataiteration'">...</xsl:when>
<xsl:when test="@activity-exectype = 'runiteration'">
<xsl:text>Run:</xsl:text>
</xsl:when>
<xsl:when test="@activity-exectype = 'runiteration'">...</xsl:when>
</xsl:choose>
<xsl:choose>...</xsl:choose>
</span>
<span class="iterationinfo {$activityclassname} ui-treeList-toggle-child">...</span>
<strong class="iterationinfo {$activityclassname} ui-treeList-toggle-child">
<xsl:value-of select="concat(@dataiterationcount, @iterationCount, @iteration)"/>
</strong>
<strong class="iterationinfo {$activityclassname} ui-treeList-toggle-child">...</strong>
</xsl:if>
<xsl:if test="@iteration-exectype or (@activity-exectype and @activity-exectype != 'execute')">...</xsl:if>
<!-- RETRY INFO TEXT -->
<xsl:if test="@retry">
<span>
<span class="ui-module-icon retry ui-treeList-toggle-child"/>
<span class="retryLabel">
<xsl:value-of select="@retry"/>
</span>
<span class="retryLabel">...</span>
</span>
<span>...</span>
</xsl:if>
<xsl:if test="@retry">...</xsl:if>
<xsl:call-template name="ActivityDescription"/>
<xsl:if test="(@result='Success' or (@result='Failed' or @result='Active')) and $max > 0">
<xsl:variable name="maxWidth" select="150"/>
<xsl:variable name="widthSuccess">
<xsl:value-of select="round(($success div $max) * $maxWidth)"/>
</xsl:variable>
<xsl:variable name="widthSuccess">...</xsl:variable>
<xsl:variable name="widthFailed">
<xsl:value-of select="round(($failed div $max) * $maxWidth)"/>
</xsl:variable>
<xsl:variable name="widthFailed">...</xsl:variable>
<xsl:if test="$widthSuccess < $maxWidth and $widthFailed < $maxWidth">
<div id="chart{@rid}" style="position: absolute; display:block; font-size:0; width:{$maxWidth}px; margin-top: 5px; right:60px; height: 8px; background-color:#777; border:1px #979797 solid; overflow: hidden; top: 6px; ">
<div style="position: absolute; width:{$widthSuccess}px; height: 8px; background-color:#42af6f;"/>
<div style="position: absolute; margin-left:{$widthSuccess}px; width:{$widthFailed}px; height: 8px; background-color:#e10000; "/>
</div>
<div id="chart{@rid}" style="position: absolute; display:block; font-size:0; width:{$maxWidth}px; margin-top: 5px; right:60px; height: 8px; background-color:#777; border:1px #979797 solid; overflow: hidden; top: 6px; ">...</div>
</xsl:if>
<xsl:if test="$widthSuccess < $maxWidth and $widthFailed < $maxWidth">...</xsl:if>
</xsl:if>
<xsl:if test="(@result='Success' or (@result='Failed' or @result='Active')) and $max > 0">...</xsl:if>
<span class="duration">
<xsl:value-of select="./@duration"/>
</span>
<span class="duration">...</span>
</h2>
<h2 class="{@result}" onclick="OnLoadContentDynamic('{@rid}','container',this);" onMouseOver="DisplayHoverMenu(this);" onMouseOut="HideHoverMenu(this)">...</h2>
<ul>
<xsl:call-template name="DetailedActivityDescription"/>
<xsl:if test="./errormessage">
<div class="errormessage">
<b class="message">
<xsl:value-of select="./errormessage"/>
</b>
<b class="message">...</b>
</div>
<div class="errormessage">...</div>
</xsl:if>
<xsl:if test="./errormessage">...</xsl:if>
<xsl:if test="./params/param">
<div class="dataparams">
<xsl:apply-templates select="./params"/>
</div>
<div class="dataparams">...</div>
</xsl:if>
<xsl:if test="./params/param">...</xsl:if>
<xsl:if test="./datarow/field">
<div class="datarow">
<xsl:apply-templates select="./datarow"/>
</div>
<div class="datarow">...</div>
</xsl:if>
<xsl:if test="./datarow/field">...</xsl:if>
<xsl:if test="$loadAll = 1">
<xsl:apply-templates select="./activity">
<xsl:with-param name="itemCount" select="$itemCount"/>
</xsl:apply-templates>
<xsl:apply-templates select="./activity">...</xsl:apply-templates>
</xsl:if>
<xsl:if test="$loadAll = 1">...</xsl:if>
